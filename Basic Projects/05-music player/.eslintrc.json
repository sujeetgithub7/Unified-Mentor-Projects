{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": "true",
    "es6": true
  },
  "extends": [
    "airbnb",
    "plugin:node/recommended",
    "prettier",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 6
  },
  "plugins": ["react"]
}
